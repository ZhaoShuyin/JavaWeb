         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>com.zsy.web.UploadServlet</servlet-class>
