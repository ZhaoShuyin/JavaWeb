<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Boot</servlet-name>
        <servlet-class>cn.zsy.web.BootServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Boot</servlet-name>
        <url-pattern>/boot</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>cn.zsy.web.UploadServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Ajax</servlet-name>
        <servlet-class>cn.zsy.ajax.AjaxServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ajax</servlet-name>
        <url-pattern>/ajax</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Ajax01</servlet-name>
        <servlet-class>cn.zsy.ajax.Ajax01Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ajax01</servlet-name>
        <url-pattern>/ajax01</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Ajax02</servlet-name>
        <servlet-class>cn.zsy.ajax.Ajax02Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ajax02</servlet-name>
        <url-pattern>/ajax02</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Agency</servlet-name>
        <servlet-class>cn.zsy.AgencyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Agency</servlet-name>
        <url-pattern>/agency</url-pattern>
    </servlet-mapping>




</web-app>